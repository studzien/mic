<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TestServiceService WSDL service -->

  <service name="TestService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.mic.agh.edu.pl"/>
      <parameter name="wsdlServiceElement" value="TestServiceService"/>
      <parameter name="schemaQualified" value="http://services.mic.agh.edu.pl"/>
      <parameter name="wsdlServicePort" value="TestService"/>
      <parameter name="className" value="pl.edu.agh.mic.services.TestServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TestService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sum" qname="operNS:sum" xmlns:operNS="http://services.mic.agh.edu.pl" returnQName="retNS:sumReturn" xmlns:retNS="http://services.mic.agh.edu.pl" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:add1" xmlns:pns="http://services.mic.agh.edu.pl" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:add2" xmlns:pns="http://services.mic.agh.edu.pl" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="multiply" qname="operNS:multiply" xmlns:operNS="http://services.mic.agh.edu.pl" returnQName="retNS:multiplyReturn" xmlns:retNS="http://services.mic.agh.edu.pl" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:mul1" xmlns:pns="http://services.mic.agh.edu.pl" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mul2" xmlns:pns="http://services.mic.agh.edu.pl" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="sum multiply"/>

  </service>
</deployment>
